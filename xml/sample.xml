<pmes>
    <workspace>/home/pmes/pmes</workspace>
    <compss>
        <installDir>/opt/COMPSs</installDir>
        <workingDir>/home/pmes</workingDir>
        <connectorJar>rocci-conn.jar</connectorJar>
        <connectorClass>es.bsc.conn.rocci.ROCCI</connectorClass>
    </compss>
    <connector className="rOCCIHelper">
        <providerName>ONE</providerName>
        <endpoint>https://rocci-server.bsc.es:11443</endpoint>
        <properties>
            <property>
                <key>providerName</key>
                <value>ONE</value>
            </property>
            <property>
                <key>endPointROCCI</key>
                <value>https://rocci-server.bsc.es:11443</value>
            </property>
            <property>
                <key>auth</key>
                <value>x509</value> 
            </property>
            <property>
                <key>ca-path</key>
                <value>/etc/grid-security/certificates/</value>
            </property>
            <property>
                <key>link</key>
                <value>/network/0</value>
            </property>
            <property>
                <key>nfs_server</key>
                <value>192.168.122.222</value>
            </property>
            <property>
                <key>infrastructure</key>
                <value>mug-bsc</value>
            </property>
            <property>
                <key>vm_java_home</key>
                <value>/usr/lib/jvm/java-8-openjdk-amd64/</value>
            </property>
            <property>
                <key>vm_compss_home</key>
                <value>/opt/COMPSs</value>
            </property> 
        </properties>
    </connector>    
    <hosts>
        <host>
            <name>bsccv14</name>
            <MAX_CPU>2400</MAX_CPU>
            <MAX_MEM>99195808</MAX_MEM>
        </host>
        <host>
            <name>bsccv15</name>
            <MAX_CPU>2400</MAX_CPU>
            <MAX_MEM>99195808</MAX_MEM>
        </host>
        <host>
            <name>bsccv16</name>
            <MAX_CPU>2400</MAX_CPU>
            <MAX_MEM>99195808</MAX_MEM>
        </host>
        <host>
            <name>bsccv17</name>
            <MAX_CPU>2400</MAX_CPU>
            <MAX_MEM>99195808</MAX_MEM>
        </host>
	<host>
            <name>bsccv18</name>
            <MAX_CPU>2400</MAX_CPU>
            <MAX_MEM>99195808</MAX_MEM>
        </host>
        <host>
            <name>bsccv19</name>          
            <MAX_CPU>2400</MAX_CPU>
            <MAX_MEM>99195808</MAX_MEM>                                    
        </host>
    </hosts>
    <logPath>/home/pmes/pmes/logs</logPath>
    <logLevel>DEBUG</logLevel>
    <timeout>60</timeout>
    <pollingInterval>5</pollingInterval>
    <runCmd>
        <cmd>echo "config script"</cmd>
    </runCmd>    
    <auth-keys>
        <key>ssh-rsa  pmes@ubuntu</key>
    </auth-keys>
</pmes>
